{
  "name": "administratie-app",
  "version": "0.1.0",
  "description": "Administratie app",
  "main": "./dist/index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "test",
    "lint": "eslint --ext=jsx,ts,tsx,js src",
    "lint:fix": "eslint --ext=jsx,ts,tsx,js src --fix",
    "start": "ts-node src/server.ts",
    "start:watch": "nodemon --exec ts-node ./src/server",
    "serve": "ts-node dist/server.js",
    "install:server": "npm install --ignore-scripts",
    "install:client": "npm install --prefix ./client",
    "postinstall": "npm install --prefix ./client",
    "build": "npm run build:server && npm run build:client",
    "build:server": "npm run lint && tsc",
    "build:client": "cd client && react-scripts build"
  },
  "nodemonConfig": {
    "ext": "js, ts",
    "ignore": [
      "src/app/bunq/*",
      "*.sqlite"
    ],
    "watch": [
      "src"
    ],
    "delay": "2500"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericjansen1988/administratie-app.git"
  },
  "keywords": [
    "administratie"
  ],
  "author": "App-Elent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericjansen1988/administratie-app/issues"
  },
  "homepage": "https://github.com/ericjansen1988/administratie-app#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "bunq-client": "^0.6.3",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "enelogic": "^0.1.7",
    "express": "^4.17.1",
    "express-collection": "^0.1.4",
    "express-sequelize-routes": "^0.1.8",
    "firebase-admin": "^8.10.0",
    "https": "^1.0.0",
    "json-store": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "morgan": "^1.10.0",
    "node-sass": "^4.13.1",
    "pg": "^7.18.2",
    "react-scripts": "^3.4.1",
    "sequelize": "^5.21.6",
    "simple-cache-js": "^0.1.1",
    "simple-encrypt-js": "^0.1.1",
    "simple-oauth2": "^3.4.0",
    "solaredge": "^0.1.7",
    "sqlite3": "^4.1.1",
    "swagger-ui-express": "^4.1.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.6",
    "@types/http-errors": "^1.6.3",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/lodash": "^4.14.149",
    "@types/moment-timezone": "^0.5.13",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.11.1",
    "@types/pg": "^7.14.3",
    "@types/simple-oauth2": "^2.5.2",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "nodemon": "^2.0.3",
    "prettier": "^1.19.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "typings": "src/typings/*.d.ts"
}
